{
  "type": "module",
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "start": "sirv public --host --port 5000",
    "lint": "npx standard --fix && npx standard --parser svelte-eslint-parser --plugin svelte '**/*.svelte' --fix",
    "start:backend": "node src/backend/obs-controller.js",
    "start:all": "concurrently \"npm run dev\" \"npm run start:backend\""
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@mdi/js": "^7.4.47",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-html": "^1.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "bulma": "^0.9.4",
    "compare-versions": "^6.1.1",
    "concurrently": "^8.2.2",
    "eslint-plugin-svelte": "^2.43.0",
    "mdi-svelte": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "obs-websocket-js": "^5.0.6",
    "postcss": "^8.4.41",
    "rollup": "^4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.2.0",
    "sass": "^1.77.8",
    "standard": "^17.1.0",
    "svelte": "^4.2.17"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "ps-list": "^8.1.1",
    "sirv-cli": "^2.0.2"
  }
}
